{
  "extends": "eslint-config-rackt",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/jsx-closing-bracket-location": [2, "after-props"],
    "react/wrap-multilines": [2, 2],
    "no-console": 2
  },
  "plugins": [
    "react"
  ]
}
