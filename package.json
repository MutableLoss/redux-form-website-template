{
  "name": "redux-form-website-template",
  "version": "0.0.1",
  "description": "The template for the redux-form website",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/redux-form"
  },
  "scripts": {
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build": "npm run clean && npm run lint && npm run build:webpack",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "react",
    "reactjs",
    "flux",
    "redux",
    "react-redux",
    "redux-form",
    "form",
    "decorator",
    "example",
    "demo"
  ],
  "author": "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikras/redux-form/issues"
  },
  "homepage": "https://redux-form.com",
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "eslint": "^1.6.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "expect": "^1.13.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "strip-loader": "^0.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9"
  },
  "npmName": "redux-form-website",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js",
        "*.css"
      ]
    }
  ]
}
